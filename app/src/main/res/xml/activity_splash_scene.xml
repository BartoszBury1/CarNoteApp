<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="6931">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="0"
               android:rotationX="0" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="10"
               android:rotationX="10" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="20"
               android:rotationX="20" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="30"
               android:rotationX="30" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="40"
               android:rotationX="40" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="50"
               android:rotationX="50" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="60"
               android:rotationX="40" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="70"
               android:rotationX="30" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="80"
               android:rotationX="20" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="90"
               android:rotationX="0" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="100"
               android:rotationX="5" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="70"
               android:alpha="0.5" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="0.38"
            android:layout_height="180dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toRightOf="parent"
            android:layout_width="180dp"
            android:id="@+id/imageView"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="20dp"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toRightOf="parent"
            android:layout_width="74dp"
            android:id="@+id/progressBar"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.596" />
        <Constraint
            motion:layout_editor_absoluteY="328dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="176dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView2" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintVertical_bias="0.38"
            android:layout_height="180dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toRightOf="parent"
            android:layout_width="180dp"
            android:id="@+id/imageView" />
        <Constraint
            android:layout_height="20dp"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toRightOf="parent"
            android:layout_width="74dp"
            android:id="@+id/progressBar"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.596"
            motion:layout_constraintHorizontal_bias="0.498" />
    </ConstraintSet>
</MotionScene>